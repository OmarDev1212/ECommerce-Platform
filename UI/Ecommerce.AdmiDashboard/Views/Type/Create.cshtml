@{
    ViewData["Title"] = "Create";

}

<div class="row justify-content-center align-content-center mt-3">
    <h1 class="col-8 my-4">Create Type</h1>
    <form method="post" asp-controller="Type" asp-action="Create" class=" row col-8">
        <div asp-validation-summary="ModelOnly"></div>
        <partial name="PartialViews/CreateEditPartialView"></partial>
        <div class="mt-4 m-auto">
            <input class="btn btn-primary" type="submit" value="Create" />
            <a asp-controller="Type" asp-action="Index" class="btn btn-outline-dark">Back to list</a>
        </div>
    </form>
</div>

@section ValidationScripts {
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}